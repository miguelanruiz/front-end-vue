     }\r\n                                  }}\r\n            }],\r\n          chartOptions: {\r\n            chart: {\r\n              height: 350,\r\n              type: 'line',\r\n              stacked: false,\r\n            },\r\n            stroke: {\r\n              width: [0, 2, 5],\r\n              curve: 'smooth'\r\n            },\r\n            plotOptions: {\r\n              bar: {\r\n                columnWidth: '50%'\r\n              }\r\n            },\r\n            fill: {\r\n              opacity: [0.85, 0.25, 1],\r\n              gradient: {\r\n                inverseColors: false,\r\n                shade: 'light',\r\n                type: \"vertical\",\r\n                opacityFrom: 0.85,\r\n                opacityTo: 0.55,\r\n                stops: [0, 100, 100, 100]\r\n              }\r\n            },\r\n            labels: () => {\r\n              var service = []\r\n              for(var i = 0; i < 15; i++){\r\n              service.push(i)\r\n              if(i==14){\r\n                return service\r\n              }\r\n            }},\r\n            markers: {\r\n              size: 0\r\n            },\r\n            xaxis: {\r\n              type: 'datetime'\r\n            },\r\n            yaxis: {\r\n              title: {\r\n                text: 'Points',\r\n              },\r\n              min: 0\r\n            },\r\n            tooltip: {\r\n              shared: true,\r\n              intersect: false,\r\n              y: {\r\n                formatter: function (y) {\r\n                  if (typeof y !== \"undefined\") {\r\n                    return y.toFixed(0) + \" points\";\r\n                  }\r\n                  return y;\r\n            \r\n                }\r\n              }\r\n            }\r\n          },\r\n            }\r\n        },\r\n        created: function(){\r\n            \r\n            axios.get('http://localhost:5002/api/tools/bracelet/report/developer/n').then(res => {\r\n                var payload = res.data\r\n                console.log(payload.length)\r\n                let bateria = []\r\n                let duracion = []\r\n                let dimension = []\r\n                for(var i = 0; i < payload.length; i++){\r\n                    bateria.push(payload[i].duracion)\r\n                    duracion.push(payload[i].ppm)\r\n                    dimension.push(payload[i].labels)\r\n                    if(i+1 == payload.length){\r\n                      this.series[0].data = bateria\r\n                      this.labels = dimension\r\n                      console.log(this.apexchart)\r\n                      this.$VueApexCharts.apexchart.updateSeries([{\r\n                        name: 'Sales',\r\n                        data: bateria\r\n                      }])\r\n                    }\r\n                }\r\n                /*this.series = {\r\n                    name: 'Bateria',\r\n                    type: 'area',\r\n                    data: payload[\"duracion\"]\r\n                }, {\r\n                    name: 'PPM',\r\n                    type: 'line',\r\n                    data: payload[\"ppm\"]\r\n                }*/\r\n            })\r\n\r\n          let user = JSON.parse(localStorage.getItem('user'));\r\n          alert(user.token);\r\n            /*axios.get('http://localhost:5002/api/search/data').then(res => {\r\n                this.users = res.data\r\n                alert(res.data)\r\n                //this.$router.push({path: '/singin'});\r\n            })*/\r\n        },\r\n        methods:{\r\n          getBatteryData(){\r\n            \r\n          }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style>\r\nbody {\r\n  font-size: .875rem;\r\n}\r\n\r\n.feather {\r\n  width: 16px;\r\n  height: 16px;\r\n  vertical-align: text-bottom;\r\n}\r\n\r\n/*\r\n * Sidebar\r\n */\r\n\r\n.sidebar {\r\n  position: fixed;\r\n  top: 0;\r\n  bottom: 0;\r\n  left: 0;\r\n  z-index: 100; /* Behind the navbar */\r\n  padding: 48px 0 0; /* Height of navba